    <div class="collection-product-grid">
      <div class="product-grid clearfix">
        {% assign bannerLoopCounter = 1 %}
        {% for product in collection.products %}
          {% if forloop.index == settings['block-1-position'] and settings['desktop-image'] %}
            {% assign image_1 = settings['desktop-image'] | img_url: "master" %}
            {% render 'PLP-image-card',
              card_index: settings['block-1-position'],
              image_src: image_1
